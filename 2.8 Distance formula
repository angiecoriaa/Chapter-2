 import java.util.*;
 
 public class DistanceFormula
 {
    public static void main(String args[]) 
    {
    int x1; int x2; int y1; int y2;
    
    Scanner sc = new Scanner(System.in);
    System.out.println("Insert x1");
    x1 = sc.nextInt();
    
    System.out.println("Insert x2");
    x2 = sc.nextInt();
    
    System.out.println("Insert y1");
    y1 = sc.nextInt();
    
    System.out.println("Insert y2");
    y2 = sc.nextInt();
    
    double xDist; double yDist;
    xDist = (x2-x1);
    yDist = (y2-y1);
    
    double distance = Math.pow(Math.pow(xDist,2)+Math.pow(yDist,2),.5);
    
    System.out.println("Distace: " + distance);
    }//End main method
}//End public class
